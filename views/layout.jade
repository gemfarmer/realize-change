	body
		div.navbar.navbar-default.navbar-fixed-top
			.container-fluid
				div.navbar-header
					a.navbar-brand(href='/') #{title}
					button.navbar-toggle(type='button', data-toggle='collapse', data-target='#navbar-main')
						span.icon-bar
						span.icon-bar
						span.icon-bar
				div.navbar-collapse.collapse#navbar-main
					ul.nav.navbar-nav
						each link in links
							- var isActive = (link.path == current ? 'active' : '')
							li(class = '#{isActive}')
								
								a(href='#{link.path}')
									i.fa(class="#{link.icon}")
									|  #{link.label}

					div.pull-right
						ul.nav.navbar-nav.navbar-right
							if username
								li.dropdown
									a.dropdown-toggle(href='#', data-toggle='dropdown')
										| Hi #{username}!&nbsp;
										b.caret
									ul.dropdown-menu
										li
											a(href='/logout') Log Out
											a(data-toggle="modal",data-target="#myModal") Account Settings

											//- a(href="/settings") Account Settings

										//- if user.role==='admin'
										//- 	li
										//- 		a(href='/admin') Admin Section
							else
								li.dropdown
									a.dropdown-toggle(href='#', data-toggle='dropdown')
										| Login
										b.caret
									ul.dropdown-menu
										li
											a(href='/auth/google') with Google
					
		div.container-fluid  
			block content

			div.navbar.navbar-fixed-bottom
				.container
					center
						form(action="https://www.paypal.com/cgi-bin/webscr",method="post",target="_top")
							input(type="hidden",name="cmd",value="_s-xclick")
							input(type="hidden",name="hosted_button_id",value="MZ8TYAL3NBYVJ")
							input(type="image",src="https://www.paypalobjects.com/en_US/i/btn/btn_donateCC_LG.gif",border="0",name="submit",alt="PayPal- The safer, easier way to pay online!")
							img(alt="",border="0",src="https://www.paypalobjects.com/en_US/i/scr/pixel.gif",width="1",height="1")
